{
  "cli": { "version": ">= 3.0.0", "appVersionSource": "remote" },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "apk": {
      "android": { "buildType": "apk" },
      "developmentClient": false,
      "distribution": "internal"
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "android": { "buildType": "app-bundle" },
      "ios": {}
    }
  }
}
